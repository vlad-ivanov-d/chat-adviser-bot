{
  "name": "chat-adviser-bot",
  "version": "1.18.0",
  "description": "Open source Telegram bot that helps to moderate chats.",
  "author": "Vlad Ivanov",
  "license": "MIT",
  "scripts": {
    "build": "eslint \"{src,apps,libs,test}/**/*.ts\" && nest build",
    "dev:env:up": "docker compose -f compose.dev.yml up --force-recreate --remove-orphans --wait -d && prisma migrate dev",
    "k6": "ts-node k6/utils/setup",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "make:badges": "istanbul-badges-readme --coverageDir=\"./coverage-total\" --silent",
    "merge:cov": "nyc report --report-dir coverage-total --reporter=json-summary --reporter=html --reporter=text-summary",
    "start": "npm run dev:env:up && nest start",
    "start:debug": "npm run dev:env:up && nest start --debug --watch",
    "start:dev": "npm run dev:env:up && nest start --watch",
    "start:prod": "node dist/main",
    "start:prod:docker": "docker compose up --build --force-recreate --remove-orphans -d",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "test:e2e:cov": "jest --config jest-e2e.config.ts --coverage",
    "test:e2e:watch": "jest --config jest-e2e.config.ts --watch",
    "test:total:cov": "npm run test:cov && npm run test:e2e:cov && npm run merge:cov && npm run make:badges"
  },
  "dependencies": {
    "@nestjs/cache-manager": "2.2.1",
    "@nestjs/common": "10.3.3",
    "@nestjs/config": "3.2.0",
    "@nestjs/core": "10.3.3",
    "@nestjs/schedule": "4.0.1",
    "@prisma/client": "5.10.2",
    "cache-manager": "5.4.0",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.0",
    "i18next": "23.9.0",
    "lodash.escaperegexp": "4.1.2",
    "nestjs-telegraf": "2.7.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "telegraf": "4.15.3"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.3",
    "@types/jest": "29.5.12",
    "@types/k6": "0.49.0",
    "@types/lodash.escaperegexp": "4.1.9",
    "@types/node": "20.11.19",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "esbuild": "0.20.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsdoc": "48.1.0",
    "eslint-plugin-optimize-regex": "1.2.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "2.1.1",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-typescript-sort-keys": "3.1.0",
    "istanbul-badges-readme": "1.8.5",
    "jest": "29.7.0",
    "k6": "0.0.0",
    "msw": "2.2.1",
    "nyc": "15.1.0",
    "prettier": "3.2.5",
    "prisma": "5.10.2",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "bugs": {
    "url": "https://github.com/vlad-ivanov-d/chat-adviser-bot/issues"
  },
  "engines": {
    "node": "20.11.1",
    "npm": "10.2.4"
  },
  "homepage": "https://github.com/vlad-ivanov-d/chat-adviser-bot#readme",
  "keywords": [
    "bot",
    "telegram",
    "moderation",
    "administration",
    "chat",
    "nodejs"
  ],
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:vlad-ivanov-d/chat-adviser-bot.git"
  },
