{
  "name": "chat-adviser-bot",
  "version": "1.29.0",
  "description": "Open source Telegram bot that helps to moderate chats.",
  "author": "Vlad Ivanov",
  "license": "MIT",
  "scripts": {
    "build": "eslint \"{fixtures,k6,libs,src,test}/**/*.ts\" && nest build",
    "dev:env:up": "docker compose up --remove-orphans --wait -d && prisma migrate dev",
    "k6": "ts-node k6/lib/setup",
    "lint": "eslint \"{fixtures,k6,libs,src,test}/**/*.ts\" --fix",
    "make:badges": "istanbul-badges-readme --coverageDir=\"./coverage-total\" --silent",
    "merge:cov": "nyc report --report-dir coverage-total --reporter=json-summary --reporter=html --reporter=text-summary",
    "start": "npm run dev:env:up && nest start",
    "start:debug": "npm run dev:env:up && nest start --debug --watch",
    "start:dev": "npm run dev:env:up && nest start --watch",
    "start:prod": "npx prisma migrate deploy && node dist/main",
    "start:prod:docker": "docker compose -f compose.build.yml up --build --remove-orphans -d",
    "test": "jest",
    "test:all": "npm run build && npm run test:cov && npm run test:e2e:cov && npm run k6 && npm run merge:cov && npm run make:badges --exitCode=1 && npm run start:dev",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "test:e2e:cov": "jest --config jest-e2e.config.ts --coverage",
    "test:e2e:watch": "jest --config jest-e2e.config.ts --watch"
  },
  "dependencies": {
    "@nestjs/cache-manager": "2.3.0",
    "@nestjs/common": "10.4.15",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.15",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/schedule": "4.1.2",
    "@prisma/client": "6.4.1",
    "cache-manager": "5.7.6",
    "cache-manager-redis-yet": "5.1.5",
    "date-fns": "3.6.0",
    "date-fns-tz": "3.1.3",
    "i18next": "24.2.2",
    "lodash.escaperegexp": "4.1.2",
    "nest-winston": "1.10.2",
    "nestjs-telegraf": "2.8.1",
    "openai": "4.86.1",
    "prebuilt-tdlib": "0.1008045.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "tdl": "8.0.2",
    "telegraf": "4.16.3",
    "winston": "3.17.0",
    "winston-loki": "6.1.3"
