    "@types/lodash.escaperegexp": "4.1.9",
    "@types/node": "22.13.8",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.25.0",
    "@typescript-eslint/parser": "8.25.0",
    "esbuild": "0.25.0",
    "eslint": "8.57.0",
