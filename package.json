{
  "name": "chat-adviser-bot",
  "version": "1.25.0",
  "description": "Open source Telegram bot that helps to moderate chats.",
  "author": "Vlad Ivanov",
  "license": "MIT",
  "scripts": {
    "build": "eslint \"{fixtures,k6,libs,src,test}/**/*.ts\" && nest build",
    "dev:env:up": "docker compose up --remove-orphans --wait -d && prisma migrate dev",
    "k6": "ts-node k6/utils/setup",
    "lint": "eslint \"{fixtures,k6,libs,src,test}/**/*.ts\" --fix",
    "make:badges": "istanbul-badges-readme --coverageDir=\"./coverage-total\" --exitCode=1 --silent",
    "merge:cov": "nyc report --report-dir coverage-total --reporter=json-summary --reporter=html --reporter=text-summary",
    "start": "npm run dev:env:up && nest start",
    "start:debug": "npm run dev:env:up && nest start --debug --watch",
    "start:dev": "npm run dev:env:up && nest start --watch",
    "start:prod": "node dist/main",
    "start:prod:docker": "docker compose -f compose.build.yml up --build --remove-orphans -d",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "test:e2e:cov": "jest --config jest-e2e.config.ts --coverage",
    "test:e2e:watch": "jest --config jest-e2e.config.ts --watch",
    "test:total": "npm run test:cov && npm run test:e2e:cov && npm run k6 && npm run merge:cov && npm run make:badges"
  },
  "dependencies": {
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "10.3.8",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.8",
    "@nestjs/platform-express": "10.3.8",
    "@nestjs/schedule": "4.0.2",
    "@prisma/client": "5.13.0",
    "cache-manager": "5.5.2",
    "cache-manager-redis-yet": "5.0.0",
    "date-fns": "3.6.0",
    "date-fns-tz": "3.1.3",
    "i18next": "23.11.3",
    "lodash.escaperegexp": "4.1.2",
    "nest-winston": "1.9.4",
    "nestjs-telegraf": "2.7.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "telegraf": "4.16.3",
    "winston": "3.13.0",
    "winston-loki": "6.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
